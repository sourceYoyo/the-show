			var idx = itemCode.indexOf(&quot;.&quot;);
			var itemCodeNew;
			if(idx != -1){
				itemCodeNew = itemCode.substring(0, idx)+&quot;\\&quot;+itemCode.substring(idx, itemCode.length);
			}else{
				itemCodeNew = itemCode;
			}
			$.ajax({
				url : &quot;/kuke/cataloguebio/praise?itemCode=&quot; + itemCode,
				type : &quot;GET&quot;,
				success : function(data){
					$(&quot;#cataPraise-&quot;+itemCodeNew).html('&lt;i&gt;&lt;/i&gt;赞（' + data + '）');
