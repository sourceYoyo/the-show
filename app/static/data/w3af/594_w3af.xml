&lt;script&gt;
	$(function (){
		//专辑点赞
		$(&quot;span[id^='cataPraise-']&quot;).on(&quot;click&quot;, function(){
			var itemCode = $(this).attr(&quot;val&quot;);
			var idx = itemCode.indexOf(&quot;.&quot;);
			var itemCodeNew;
			if(idx != -1){
				itemCodeNew = itemCode.substring(0, idx)+&quot;\\&quot;+itemCode.substring(idx, itemCode.length);
			}else{
				itemCodeNew = itemCode;
			}
			$.ajax({
				url : &quot;/kuke/cataloguebio/praise?itemCode=&quot; + itemCode,
				type : &quot;GET&quot;,
				success : function(data){
					$(&quot;#cataPraise-&quot;+itemCodeNew).html('&lt;i&gt;&lt;/i&gt;赞（' + data + '）');
					$(&quot;#cataPraise-&quot;+itemCodeNew).addClass(&quot;goodhover&quot;);
					$(&quot;#cataPraise-&quot;+itemCodeNew).unbind(&quot;click&quot;);
				},
				error : function(){
					alert(&quot;网络错误！&quot;);
				}
			});
		});
		//杂志点赞  
		$(&quot;span[id^='magaZinePraise-']&quot;).on(&quot;click&quot;,function(){
			var _this=this;
			var praiseTimes=$(this).attr(&quot;praiseTimes&quot;);
			var praise=parseInt(praiseTimes)+1;
			var magaZineId = $(this).attr(&quot;magaZineId&quot;);
			$.ajax({
				url : &quot;/kuke/magaZine/update&quot;,
				type : &quot;GET&quot;,
				data : &quot;id=&quot;+magaZineId+&quot;&amp;praiseTimes=&quot;+praise,
				success : function(data){
					$(_this).unbind(&quot;click&quot;); //取消单击事件
					$(&quot;#magaZinePraise-&quot;+magaZineId).removeClass(&quot;comlist2-good&quot;);
					$(&quot;#magaZinePraise-&quot;+magaZineId).addClass(&quot;comlist2-good goodhover1&quot;);
				},
				error : function(){
					alert(&quot;网络错误！&quot;);
				}
			}); 
		});
		
		//播放主题
		$(&quot;.themeplay&quot;).click(function(){
			var themeid = $(this).attr(&quot;themeid&quot;);
			playTheme(themeid);
		});
		
		//专辑播放
		$(&quot;a[id^='cataplay-']&quot;).click(function(){
			var itemcode = $(this).attr(&quot;itemcode&quot;);
			playCata(itemcode);
		});
		
		//顶部菜单亮 
		$(&quot;.topnav clear a&quot;).removeClass(&quot;on wd01&quot;);
		$(&quot;#amenu0&quot;).addClass(&quot;on wd01&quot;);
		
		//弹层关闭 
		$(&quot;#closeUpload&quot;).on(&quot;click&quot;,function(){
			$(&quot;.popupbg,.popup&quot;).hide();
		});
		
		// 上传课件弹层
		$(&quot;#uploadFile&quot;).on(&quot;click&quot;,function(){
			$(&quot;.popupbg,.popup&quot;).show();
		});
		
		$(&quot;.popupbg&quot;).on(&quot;click&quot;,function(){
			$(this).hide();
			$(&quot;.popup&quot;).hide();
		});
		
		// 上传课件
		$(&quot;.kejian-file&quot;).on(&quot;change&quot;,function(){
			var kjValue = $(this).val();
			$(&quot;.kejian-upload&quot;).val(kjValue);
		});
		
		// 上传图片
		$(&quot;.img-file&quot;).on(&quot;change&quot;,function(){
			var imgValue = $(this).val();
			$(&quot;.img-upload&quot;).val(imgValue);
		});
		
		//上传文件判断
		//判断课件名称长度
		$(&quot;#courseWareName&quot;).blur(function(){
			var name = $.trim($(&quot;#courseWareName&quot;).val());
			if(name.length&gt;30){
				alert(&quot;课件名称不能超过30个字&quot;);
			}
			if(name.length==0){
				alert(&quot;请输入课件名称&quot;);
			}
		});
		//判断课件简介长度
		$(&quot;#courseWareDesc&quot;).blur(function(){
			var desc = $.trim($(this).val());
			if(desc.length&lt;10){
				alert(&quot;课件简介不得少于10字&quot;);
			}
		});
		//判断上传人名称
		$(&quot;#courseWareUploader&quot;).blur(function(){
			var uploader = $.trim($(this).val());
			if(uploader.length==0){
				alert(&quot;请输入上传人姓名&quot;);
			}
		});
		//判断学校
		$(&quot;#courseWareSchool&quot;).blur(function(){
			var school = $.trim($(this).val());
			if(school.length==0){
				alert(&quot;请输入学校&quot;);
			}
		});
		//判断所属地
		$(&quot;#Province&quot;).blur(function(){
			var province = $.trim($(this).val());
			if(province.length==0){
				alert(&quot;请输入上传人姓名&quot;);
			}
		});
		//判断文件大小，不能超过30M
		$(&quot;#file&quot;).on(&quot;change&quot;,function(){
			var fileSize = this.files[0].size;
			$(this).data('filesize', fileSize);
			if(fileSize&gt;30*1024*1024){
				alert(&quot;文件大小不能超过30M&quot;);
			}
		});
		//判断上传封面，不能超过2M
		$(&quot;#image&quot;).on(&quot;change&quot;,function(){
			var fileSize = this.files[0].size;
			$(this).data('filesize', fileSize);
			if(fileSize&gt;2*1024*1024){
				alert(&quot;文件大小不能超过2M&quot;);
			}
		});

		$(&quot;#sub&quot;).unbind(&quot;click.sub&quot;).on(&quot;click.sub&quot;,function(){
			var name = $.trim($(&quot;#courseWareName&quot;).val());
			var desc = $.trim($(&quot;#courseWareDesc&quot;).val());
			var uploader = $.trim($(&quot;#courseWareUploader&quot;).val());
			var school = $.trim($(&quot;#courseWareSchool&quot;).val());
			var province = $.trim($(&quot;#Province&quot;).val());
			var filesize = $(&quot;#file&quot;).data('filesize');
			var imgSize = $(&quot;#image&quot;).data('filesize');
			if(name.length&gt;30||desc.length&lt;10||imgSize==null||imgSize&gt;2*1024*1024||filesize==null||filesize&gt;30*1024*1024||uploader.length==0||school.length==0||province.length==0){
				alert(&quot;请核对填入的信息&quot;);
			}else{
				$(&quot;#form&quot;).submit();
			}
		});
		
	});

	function G(id){
		return document.getElementById(id);
	}
	function changeSpanValue(value,id){
		if(value != ''){
			G(id).innerHTML = value;
		}
	}
