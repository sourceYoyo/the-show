&lt;script type=&quot;text/javascript&quot;&gt;



	$(function(){		
$(&quot;.bd li&quot;).hide()

		var dq_date = new Date();
		var month = dq_date.getMonth()+1;
		var day = dq_date.getDate();
		if(month&lt;10){
			month= &quot;0&quot;+month;
		}
		if(day&lt;10){
			day = &quot;0&quot;+day;
		}
		var img_date= &quot;.date&quot;+ dq_date.getFullYear()+ month;
		$(&quot;.menu_con h1 i&quot;).text(dq_date.getFullYear()+&quot;-&quot;+month+&quot;-&quot;+day)
		dq_date =&quot;#date&quot;+ dq_date.getFullYear()+ month;
		$(dq_date).addClass(&quot;act&quot;);
		$(img_date).addClass(&quot;on&quot;);
		$(&quot;.month i&quot;).text($(dq_date).attr(&quot;rel&quot;));


		
		$(&quot;ul.date&quot;).each(function(index, element) {
			var year =$(this).attr(&quot;id&quot;).substring(4,8);
			var month =$(this).attr(&quot;id&quot;).substring(8,$(this).attr(&quot;id&quot;).length);
			if(month.length&lt;2){
				month = &quot;0&quot;+month;
			}
			var day = DayNumOfMonth(year,month);
			var dq_day;
			for(var i=0;i&lt;day;i++){
				var html='';
				if(i&lt;10){
					dq_day = &quot;0&quot;+(i+1);
				}else{
					dq_day =(i+1);
				}
				html +=&quot;&lt;li id='&quot;+year+month+dq_day+&quot;'&gt;&quot;+(i+1)+&quot;&lt;/li&gt;&quot;
				$(html).appendTo($(this))
			}

		});



		$(&quot;.slideBox li&quot;).each(function(index, element) {
			$(&quot;#&quot;+$(this).attr(&quot;rel&quot;)).addClass(&quot;on&quot;)
		});
		$(&quot;.time ul&quot;).each(function(index, element) {
			$(this).find(&quot;.on&quot;).first().addClass(&quot;now&quot;);
		});

		
	
		
	});

	$(&quot;.ld_spanR&quot;).click(function(e) {
		$(&quot;.on .playState&quot;).click();
		//$(&quot;.on .slideBox&quot;).slide({autoPlay:false});
		if($(&quot;.sroll_img .on&quot;).index()!=$(&quot;.sroll_img dd&quot;).length-1){
			$(&quot;.sroll_img .on&quot;).removeClass(&quot;on&quot;).next().addClass(&quot;on&quot;);
			$(&quot;.time .act&quot;).removeClass(&quot;act&quot;).next().addClass(&quot;act&quot;);
			$(&quot;.month i&quot;).text($(&quot;.time .act&quot;).attr(&quot;rel&quot;))
			$(&quot;.on .slideBox&quot;).slide({mainCell:&quot;.bd ul&quot;,trigger:&quot;click&quot;,autoPlay:true,delayTime:2000,interTime:5000,effect:&quot;fold&quot;,
				startFun:function(i,c){
					if(i&gt;0&amp;&amp;i!=c){
						$(&quot;.time .act .on&quot;).eq(i).addClass(&quot;now&quot;).siblings().removeClass(&quot;now&quot;);
					}else{
						$(&quot;.time .act .now&quot;).removeClass(&quot;now&quot;);
						$(&quot;.time .act .on&quot;).first().addClass(&quot;now&quot;)
					}
				}
			});
			}
		});
	
	$(&quot;.ld_spanL&quot;).click(function(e) {
		$(&quot;.on .playState&quot;).click();
