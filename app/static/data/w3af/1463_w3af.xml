			if(month.length&lt;2){
				month = &quot;0&quot;+month;
			}
			var day = DayNumOfMonth(year,month);
			var dq_day;
			for(var i=0;i&lt;day;i++){
				var html='';
				if(i&lt;10){
					dq_day = &quot;0&quot;+(i+1);
				}else{
					dq_day =(i+1);
				}
				html +=&quot;&lt;li id='&quot;+year+month+dq_day+&quot;'&gt;&quot;+(i+1)+&quot;&lt;/li&gt;&quot;
