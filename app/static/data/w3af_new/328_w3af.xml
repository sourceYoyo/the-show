    &lt;section id=&quot;pubs&quot; class=&quot;about-section section-first&quot;&gt;
        &lt;div class=&quot;block-v1  bg-grey&quot;&gt;
            &lt;div class=&quot;container &quot;&gt;
                &lt;div class=&quot;title-v1&quot;&gt;
                    &lt;h1&gt;Publications&lt;/h1&gt;
                &lt;/div&gt;

                &lt;div class=&quot;row&quot; id=&quot;kdd&quot;&gt;
                    &lt;div class=&quot;col-lg-12&quot;&gt;
                        &lt;h4 class=&quot;text-muted&quot;&gt;COSNET: Connecting Heterogeneous Social Networks with Local and Global Consistency&lt;/h4&gt;
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                        &lt;b&gt;Yutao Zhang&lt;/b&gt;, Jie Tang, Zhilin Yang, Jian Pei, Philip Yu
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                        To Appear In Proceedings of ACM SIGKDD International Conference on Knowledge Discovery and Data Mining (KDD), 2015
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                        Oral presentation, acceptance rate 19%.
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12&quot;&gt;
                        [&lt;a target=&quot;_blank&quot; href=&quot;./papers/zhang-kdd-2015.pdf&quot;&gt;PDF&lt;/a&gt;] [&lt;a target=&quot;_blank&quot; href=&quot;./slides/kdd-slides.pdf&quot;&gt;Slides&lt;/a&gt;]
                    &lt;/div&gt;
                &lt;/div&gt;

                &lt;br&gt;&lt;br&gt;

                 &lt;div class=&quot;row&quot;&gt;
                    &lt;div class=&quot;col-lg-12&quot;&gt;
                        &lt;h4 class=&quot;text-muted&quot;&gt;EgoNetCloud: Event-based Egocentric Dynamic Network Visualization&lt;/h4&gt;
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                        Qingsong Liu, Yifan Hu, Lei Shi, Xinzhu Mu, &lt;b&gt;Yutao Zhang&lt;/b&gt;, Jie Tang
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                        IEEE Visual Analytics Science and Technology (VAST)
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12 text-muted&quot;&gt;
                    &lt;/div&gt;
                    &lt;div class=&quot;col-md-12&quot;&gt;
                        [&lt;a target=&quot;_blank&quot; href=&quot;./papers/yang-wsdm-2014.pdf&quot;&gt;PDF&lt;/a&gt;]
                    &lt;/div&gt;
